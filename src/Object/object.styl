/**
 * Layout
 */
.attributeObject
.attributeObjectToggle
.attributeObjectMembers
.attributeObjectMemberContainer
.attributeObjectDefaults
  width auto
  height auto
  padding 0
  margin 0

.attributeObject
  display flex
  flex-direction row
  flex-wrap wrap
  justify-content flex-start
  align-items flex-start

.attributeObjectMembers
.attributeObjectDefaults
  width 100%
  height auto

.attributeObjectDefaults
  border-top 1px solid #d8dbe4

.attributeObject
  > .attributeObjectMembers
    > .attributeObjectMemberContainer
      > .attributeObjectMember.isExpandableCollapsible
        > .attributeObjectMemberValueRow
          > .attributeObjectMemberValue
            > .attributeObject
              > .attributeObjectDefaults
                padding-left 10px
                padding-right 10px
                padding-bottom 10px
