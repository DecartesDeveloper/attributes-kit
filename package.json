{
  "name": "attributes-kit",
  "version": "1.0.0",
  "description": "React component for MSON rendering",
  "main": "src/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --progress --colors",
    "generateFixtures": "node ./scripts/run-generate",
    "lint": "eslint src --ext .js,.es6 src",
    "playground": "shell-exec 'npm run start' 'node ./scripts/run.js ./launch-browser http://localhost:8080/'",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run unitTests",
    "start": "node ./scripts/run.js ./dev-server",
    "unitTests": "mocha --compilers js:babel/register ./src/**/test/*.js --recursive",
    "visual-testing": "NODE_ENV=test shell-exec 'npm run start' 'node ./scripts/run.js ./launch-browser http://localhost:8080/visual-testing.html'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apiaryio/attributes-kit.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apiaryio/attributes-kit/issues"
  },
  "homepage": "https://github.com/apiaryio/attributes-kit#readme",
  "dependencies": {
    "classnames": "^2.1.3",
    "minim": "^0.10.0",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "async": "^1.4.2",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "body-parser": "^1.13.3",
    "browser-launcher2": "^0.4.6",
    "css-loader": "^0.17.0",
    "dedent": "^0.4.0",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.3.1",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "flux": "^2.1.1",
    "jquery": "^2.1.4",
    "json-formatter-js": "^0.2.0",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.3.2",
    "mson-zoo": "^2.0.0",
    "nib": "^1.1.0",
    "protagonist": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "shell-executor": "^0.3.2",
    "style-loader": "^0.12.3",
    "stylus-loader": "1.2.x",
    "superagent": "^1.3.0",
    "svgo": "^0.5.6",
    "underscore": "^1.8.3",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  }
}
